{
  "name": "callcenteranalytics",
  "version": "1.0.0",
  "description": "call center analytics modern dashboard",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --hot",
    "deploy": "better-npm-run deploy",
    "build": "react-scripts build",
    "deploy:dev": "npm run clean && better-npm-run deploy:dev",
    "deploy:prod": "npm run clean && better-npm-run deploy:prod",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "eslint": "eslint src"
  },
  "betterScripts": {
    "compile": {
      "command": "node compile/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon compile/server --ignore dist --ignore coverage --ignore tests --ignore src",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node compile/server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/compile/karma start build/karma.conf",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.11.6",
    "connect-history-api-fallback": "^1.5.0",
    "css-loader": "^0.28.10",
    "cssnano": "^3.10.0",
    "debug": "^3.1.0",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "1.0.0",
    "file-loader": "^1.1.11",
    "fs-extra": "0.30.0",
    "html-webpack-plugin": "2.22.0",
    "ip": "^1.1.5",
    "json-loader": "^0.5.7",
    "postcss-loader": "0.13.0",
    "sass-loader": "4.0.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.6",
    "webpack": "^1.15.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-sources": "^1.1.0",
    "yargs": "^11.0.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "better-npm-run": "^0.1.0",
    "bootstrap": "4.0.0",
    "d3": "^3.5.17",
    "es6-promise": "^4.2.4",
    "lodash": "^4.17.5",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.21.0",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.1",
    "react-color": "^2.14.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dock": "^0.2.4",
    "react-dom": "^16.2.0",
    "react-input-autosize": "^2.2.1",
    "react-input-mask": "^1.2.2",
    "react-paginate": "^5.1.0",
    "react-redux": "^5.0.7",
    "react-redux-toastr": "^7.2.3",
    "react-rnd": "^7.3.1",
    "react-router": "^3.2.0",
    "react-router-redux": "^4.0.8",
    "react-scrollbar": "^0.5.4",
    "react-select": "^2.0.0-alpha.8",
    "react-toastr": "^3.0.0",
    "react-widgets": "^4.2.2",
    "react-widgets-moment": "^4.0.9",
    "reactcss": "^1.2.3",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "url-search-params-polyfill": "^2.0.1"
  }
}
